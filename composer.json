{
    "name": "amphp/postgres",
    "description": "Asynchronous PostgreSQL client for Amp.",
    "keywords": [
        "database",
        "db",
        "postgresql",
        "postgre",
        "pgsql",
        "asynchronous",
        "async"
    ],
    "homepage": "http://amphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "amphp/amp": "^2.0"
    },
    "require-dev": {
        "amphp/phpunit-util": "dev-master as 0.1",
        "phpunit/phpunit": "^6",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Amp\\Postgres\\": "lib"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Postgres\\Test\\": "test"
        }
    }
}
